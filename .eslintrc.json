{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "es2021": true,
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "camelcase": "off",
        "no-console": "off",
        "no-plusplus": "off",
        "lines-between-class-members": "off",
    }
}
